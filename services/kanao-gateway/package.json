{
  "name": "@nezuchan/kanao-gateway",
  "version": "5.4.2",
  "description": "A standalone service for connecting to the Discord gateway.",
  "license": "GPL-3.0",
  "author": "KagChi",
  "type": "module",
  "main": "dist/index.js",
  "private": "true",
  "scripts": {
    "build": "rimraf dist && swc ./src -d dist --config-file .swcrc --strip-leading-paths && node -p \"fs.cpSync('./drizzle', './dist/drizzle', { recursive: true })\"",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "start": "node --expose-gc -r dotenv/config dist/index.js",
    "start:dev": "npm run build && node --expose-gc -r dotenv/config dist/index.js"
  },
  "dependencies": {
    "@discordjs/collection": "^2.1.0",
    "@discordjs/rest": "^2.3.0",
    "@discordjs/ws": "^1.1.1",
    "@nezuchan/constants": "^0.9.1",
    "@nezuchan/utilities": "^0.7.0",
    "@sapphire/pieces": "^4.2.2",
    "@sapphire/result": "^2.6.6",
    "@sapphire/time-utilities": "^1.7.12",
    "@sapphire/utilities": "^3.16.2",
    "@skyra/start-banner": "^2.0.1",
    "@types/better-sqlite3": "^7.6.10",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "better-sqlite3": "^11.0.0",
    "bufferutil": "^4.0.8",
    "discord-api-types": "^0.37.90",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "gradient-string": "^2.0.2",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.1",
    "prometheus-middleware": "^1.3.6",
    "tslib": "^2.6.3",
    "utf-8-validate": "^6.0.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.6.3",
    "@types/amqplib": "^0.10.5",
    "@types/dockerode": "^3.3.29",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^20.14.7",
    "@vladfrangu/async_event_emitter": "^2.2.4",
    "drizzle-kit": "^0.22.7",
    "rimraf": "^5.0.7",
    "typescript": "^5.5.2"
  },
  "optionalDependencies": {
    "ioredis": "^5.4.1",
    "pino-loki": "^2.3.0",
    "zlib-sync": "^0.1.9"
  }
