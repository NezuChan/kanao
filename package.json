{
  "name": "@nezuchan/nezu-gateway",
  "version": "3.1.0",
  "description": "A standalone service for connecting to the Discord gateway.",
  "license": "GPL-3.0",
  "author": "KagChi",
  "type": "module",
  "main": "dist",
  "scripts": {
    "build": "rimraf dist && swc src -d dist",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --fix --ext ts",
    "start": "npm run build && node -r dotenv/config dist/index.js"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "@hazmi35/eslint-config/typescript"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off",
      "class-methods-use-this": "off",
      "@typescript-eslint/restrict-template-expressions": "off",
      "@typescript-eslint/naming-convention": "off"
    }
  },
  "dependencies": {
    "@discordjs/collection": "^2.0.0",
    "@discordjs/rest": "^2.2.0",
    "@discordjs/ws": "^1.0.2",
    "@nezuchan/constants": "^0.7.0",
    "@nezuchan/utilities": "^0.6.2",
    "@sapphire/pieces": "^4.2.0",
    "@sapphire/result": "^2.6.5",
    "@sapphire/time-utilities": "^1.7.11",
    "@sapphire/utilities": "^3.15.1",
    "@skyra/start-banner": "^2.0.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "bufferutil": "^4.0.8",
    "discord-api-types": "^0.37.67",
    "dockerode": "^4.0.2",
    "dotenv": "^16.3.1",
    "gradient-string": "^2.0.2",
    "ioredis": "^5.3.2",
    "kearsarge": "^1.0.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "prometheus-middleware": "^1.3.2",
    "tslib": "^2.6.2",
    "utf-8-validate": "^6.0.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@hazmi35/eslint-config": "^11.0.0",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.102",
    "@types/amqplib": "^0.10.4",
    "@types/dockerode": "^3.3.23",
    "@types/gradient-string": "^1.1.5",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vladfrangu/async_event_emitter": "^2.2.4",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "pino-loki": "^2.1.3",
    "zlib-sync": "^0.1.9"
  }
}
