{
  "name": "@nezuchan/nezu-gateway",
  "version": "1.9.2",
  "description": "A standalone service for connecting to the Discord gateway.",
  "main": "dist",
  "scripts": {
    "start": "npm run build && node -r dotenv/config dist/index.js",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --fix --ext ts",
    "build": "rimraf dist && swc src -d dist"
  },
  "type": "module",
  "author": "KagChi",
  "license": "GPL-3.0",
  "devDependencies": {
    "@hazmi35/eslint-config": "^8.7.1",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.27",
    "@types/gradient-string": "^1.1.2",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vladfrangu/async_event_emitter": "^2.1.3",
    "eslint": "^8.33.0",
    "rimraf": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "@hazmi35/eslint-config/typescript"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off"
    }
  },
  "dependencies": {
    "@discordjs/collection": "^1.3.0",
    "@discordjs/rest": "^1.5.0",
    "@discordjs/util": "^0.1.1-dev.1674173459-8b70f49.0",
    "@discordjs/ws": "^0.6.1-dev.1674129868-56ef3ad.0",
    "@nezuchan/cordis-brokers": "^2.0.0",
    "@nezuchan/redis-collection": "^1.3.0",
    "@sapphire/async-queue": "^1.5.0",
    "@sapphire/pieces": "^3.6.0",
    "@sapphire/result": "^2.6.0",
    "@sapphire/time-utilities": "^1.7.8",
    "@sapphire/utilities": "^3.11.0",
    "@skyra/start-banner": "^1.0.2",
    "@types/amqplib": "^0.10.1",
    "discord-api-types": "^0.37.28",
    "dotenv": "^16.0.3",
    "gradient-string": "^2.0.2",
    "ioredis": "^5.2.5",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "prometheus-middleware": "^1.1.2",
    "zlib-sync": "^0.1.7"
  }
}
